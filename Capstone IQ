#pragma config(Sensor, port2,  startUp,        sensorVexIQ_Touch)
#pragma config(Sensor, port3,  feedbackLED,    sensorVexIQ_LED)
#pragma config(Sensor, port6,  colorSense,     sensorVexIQ_Color12Color)
#pragma config(Motor,  motor1,          seesaw,        tmotorVexIQ, PIDControl, encoder)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//
string y;
//= getColorMode(colorSense);
//int x = getColorName(colorSense);
string g;
//= getColorMode(colorSense);

string z;
//= getColorName(colorSense);
#include "jobsIQ_NEW.h"
task main()
{
	y = getColorMode(colorSense);
	g = getColorMode(colorSense);
	z = getColorName(colorSense);
	sensorInit();
	delay(100);
	while(true){
		getColorMode(colorSense);
		if(getBumperValue(startUp) == 1){
			startTask(sortSide);
		}
		if(z == "\x05"){
			setTouchLEDColor(feedbackLED, colorYellow);
		}
	}
}
